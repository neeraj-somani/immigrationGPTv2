{
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/main.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "TAVILY_API_KEY": "@tavily_api_key",
    "LANGCHAIN_API_KEY": "@langchain_api_key"
  }
}
